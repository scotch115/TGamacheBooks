{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "functions/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
		"rewrites": [{
			"source": "**/**",
			"function": "app"
			}],
		"cleanUrls": true
  },
  "storage": {
    "rules": "storage.rules"
  }
}
